<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="demon.com.androidnet.SocketActivity">

    <ImageView
        android:id="@+id/iv_url"
        android:layout_width="100dp"
        android:layout_height="100dp" />

    <Button
        android:id="@+id/url"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="url" />

    <TextView
        android:id="@+id/tv_urlConnect"
        android:layout_width="match_parent"
        android:layout_height="100dp" />

    <Button
        android:id="@+id/urlConnect"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="url" />

    <TextView
        android:id="@+id/tv_httpurlconnect"
        android:layout_width="match_parent"
        android:layout_height="100dp" />

    <Button
        android:id="@+id/httpurlconnect"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="httpurlconnect" />


</LinearLayout>
